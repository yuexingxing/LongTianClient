<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/item_layout_receivedetail_billcode"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="单号编号" />

        <TextView
            android:id="@+id/item_layout_receivedetail_sitename"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="网点" />

        <TextView
            android:id="@+id/item_layout_receivedetail_count"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="件数" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="总重量" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="总体积" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="贷款" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="运费" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="支付方式" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="到付款" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="回单号" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="寄件人" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="寄件电话" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="收件人" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="收件电话" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="签收人" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="签收网点" />

        <TextView
            android:id="@+id/item_layout_scanrecord_content"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@drawable/shape_rec_black"
            android:gravity="center"
            android:text="签收时间" />
    </LinearLayout>

</LinearLayout>